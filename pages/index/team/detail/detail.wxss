/* pages/index/team/detail/detail.wxss */

page {
  height: 100%;
}

.myteam-list, .search-team, .public-team {
  height: 100rpx;
  padding: 10rpx 40rpx;
  width: 100%;
  background: #fff;
  margin-bottom: 10rpx;
}

.myteam-item, .search-content, .public-content {
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
}

.myteam-item view, .public-content view {
  float: left;
}

/*  战队详情  */

.team-detail-icon {
  width: 100%;
  height: 204rpx;
  background: #000;
  padding: 20rpx 0rpx;
}

.team-detail-icon .image-content {
  width: 164rpx;
  height: 164rpx;
  margin: 0 auto;
}

/* member  */

.member {
  height: 80rpx;
  padding: 10rpx 40rpx;
  width: 100%;
  background: #fff;
  margin-bottom: 10rpx;
}

.member-content {
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
}

.meber-list {
  background-color: #fff;
  width: 100%;
  position: absolute;
  top: 398rpx;
  bottom: 0px;
  left: 0px;
}
